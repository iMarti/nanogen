<!DOCTYPE html>
<html lang="en-US">
<head>
  <%- include('partials/head') %>
</head>
<body>
<section class="page-header">
  <h1 class="project-name"><%= page.title %></h1>
</section>

<section class="main-content">
  <%- include('partials/breadcrumb') %>
  <%- contents.body %>
  
  <% if (contents.excerpt) { %>
  <div class="excerpt"><%- contents.excerpt %></div>
  <% } %>

  <p>Back to <a href="<%= page.parent.url %>"><%= page.parent.menuTitle %></a></div>

  <h3>Other books</h3>
  <p>
    <% page.siblings.forEach(sibling => { %>
      <a class="btn <%= page === sibling ? 'active' : ''%>" href="<%= sibling.url %>"><%= sibling.title %></a>
    <% }) %>
  </p>

  <footer class="site-footer">
    <span class="site-footer-credits">This page was generated by <a href="https://github.com/iMarti/nanogen">NanoGen</a>.</span>
  </footer>
</section>
</body>
</html>
